{
	"semi": true,
	"singleQuote": true,
	"printWidth": 120,
	"useTabs": true,
	"tabWidth": 4,
	"trailingComma": "all",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"proseWrap": "never",
	"htmlWhitespaceSensitivity": "strict",
	"endOfLine": "lf",
	"singleAttributePerLine": true
}
